@model IEnumerable<ATM.Models.Project>
@{
    ViewBag.Title = "پروژه ها";
}

<div class="index">
    @Html.ActionLink("• پروژه ها", "Index", null, new { @class = "title" })
    <table>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Start)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.End)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Title)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DateDimension.DateKey)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DateDimension1.DateKey)
                </td>
                <td>
                    @Html.ActionLink("مشاهده", "Details", new { id = item.Id })
                    @Html.ActionLink("ویرایش", "Edit", new { id = item.Id })
                    @Html.ActionLink("حذف", "Delete", new { id = item.Id })
                </td>
            </tr>
        }
    </table>
    @Html.ActionLink("پروژه جدید", "Create")
</div>
